{
  "name": "core",
  "sourceRoot": "libs/core/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@monodon/rust:build",
      "outputs": ["{projectRoot}/target"],
      "options": {
        "toolchain": "stable",
        "package": "reqsmith-core"
      }
    },
    "test": {
      "executor": "@monodon/rust:test",
      "options": {
        "toolchain": "stable",
        "package": "reqsmith-core"
      }
    },
    "integration-test": {
      "executor": "@monodon/rust:test",
      "options": {
        "toolchain": "stable",
        "package": "reqsmith-core",
        "testBinary": "integration_tests"
      }
    },
    "test:file-watcher": {
      "executor": "@monodon/rust:test",
      "options": {
        "toolchain": "stable",
        "package": "reqsmith-core",
        "testFilter": "file_watcher"
      }
    },
    "clippy": {
      "executor": "@monodon/rust:clippy",
      "options": {
        "toolchain": "stable",
        "package": "reqsmith-core",
        "fix": false
      }
    }
  },
  "tags": ["scope:backend", "type:lib", "lang:rust"]
}
